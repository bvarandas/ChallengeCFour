{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "QueueCommandSettings": {
    "Hostname": "localhost",
    "QueueName": "QueueCommandCashFlow",
    "ExchangeService": "cashflow-service-command",
    "ExchangeType": "fanout",
    "RoutingKey": "cash-flow-command",
    "port": 5672,
    "Interval": 5000
  },
  "QueueEventSettings": {
    "Hostname": "localhost",
    "QueueName": "QueueEventCashFlow",
    "ExchangeService": "cashflow-service-event",
    "ExchangeType": "fanout",
    "RoutingKey": "cash-flow-event",
    "port": 5672,
    "Interval": 5000
  },
  "CashFlowStoreDatabase": {
    "ConnectionString": "mongodb://root:example@mongo:27017",
    "DatabaseName": "CashFlowStore",
    "CashFlowCollectionName": "CashFlow"
  }
}