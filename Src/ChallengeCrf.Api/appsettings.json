{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "QueueCommandSettings": {
    //"Hostname": "rabbitmq",
    "Hostname": "localhost",
    "QueueName": "QueueCommandCashFlow",
    "ExchangeService": "cashflow-service-command",
    "ExchangeType": "direct",
    "RoutingKey": "cash-flow-command",
    "port": 5672,
    "Interval": 5000
  },
  "QueueEventSettings": {
    //"Hostname": "rabbitmq",
    "Hostname": "localhost",
    "QueueName": "QueueEventCashFlow",
    "ExchangeService": "cashflow-service-event",
    "ExchangeType": "direct",
    "RoutingKey": "cash-flow-event",
    "port": 5672,
    "Interval": 5000
  }
  
}
